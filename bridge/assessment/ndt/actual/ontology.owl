    <!-- https://w3id.org/ndto#FrequencyMeasurement -->


    <owl:Class rdf:about="https://w3id.org/ndto#FrequencyMeasurement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#NDTMeasurement"/>
        <rdfs:comment xml:lang="en">Frequency measurement</rdfs:comment>
