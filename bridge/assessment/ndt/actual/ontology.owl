        <rdfs:label xml:lang="en">Force</rdfs:label>
        <rdfs:label xml:lang="es">Fuerza</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#Frequency -->


    <owl:Class rdf:about="https://w3id.org/ndto#Frequency">
        <rdfs:subClassOf rdf:resource="http://qudt.org/schema/qudt/Quantity"/>
        <qudt:quantityKind rdf:resource="http://qudt.org/vocab/quantitykind/Frequency"/>
