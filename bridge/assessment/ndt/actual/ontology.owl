        <rdfs:label xml:lang="en">Frequency Measurement</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#HelixScrewPullOutTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#HelixScrewPullOutTest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#PullOutResistanceTest"/>
        <rdfs:comment xml:lang="en">Helix-Screw pull-out test</rdfs:comment>
        <rdfs:label xml:lang="es">Ensayo de Extracción de Helicoidal</rdfs:label>
        <rdfs:label xml:lang="en">Helix-Screw Pull-Out Test</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#ImpactEchoTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#ImpactEchoTest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#ElasticWavePropagationTest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#FrequencyMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#PWaveVelocityMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Impact-echo tests are used for estimating the thickness of elements and detecting the presence of cracks and voids through frequency analysis</rdfs:comment>
        <rdfs:label xml:lang="es">Ensayo de Eco de Impacto</rdfs:label>
        <rdfs:label xml:lang="en">Impact-Echo Test</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#InfraredThermography -->


    <owl:Class rdf:about="https://w3id.org/ndto#InfraredThermography">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#ThermalTest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#TemperatureMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Uses infrared cameras to detect temperature variations indicating structural defects</rdfs:comment>
        <rdfs:label xml:lang="en">Infrared Thermography</rdfs:label>
        <rdfs:label xml:lang="es">Termografía Infrarroja</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#Material -->


    <owl:Class rdf:about="https://w3id.org/ndto#Material">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:comment xml:lang="en">Generalization of the Materials tested using NDT methods</rdfs:comment>
        <rdfs:label xml:lang="en">Material</rdfs:label>
        <rdfs:label xml:lang="es">Material</rdfs:label>
