    <!-- http://qudt.org/schema/qudt/Quantity -->


    <owl:Class rdf:about="http://qudt.org/schema/qudt/Quantity"/>
    


    <!-- http://www.w3.org/ns/sosa/Observation -->


    <owl:Class rdf:about="http://www.w3.org/ns/sosa/Observation"/>
    


    <!-- https://w3id.org/ndto#CompressiveStrength -->


    <owl:Class rdf:about="https://w3id.org/ndto#CompressiveStrength">
        <rdfs:subClassOf rdf:resource="http://qudt.org/schema/qudt/Quantity"/>
        <qudt:quantityKind rdf:resource="http://qudt.org/vocab/quantitykind/CompressiveStress"/>
        <rdfs:comment xml:lang="en">Compressive strength</rdfs:comment>
        <rdfs:comment xml:lang="es">Resistencia a la compresión</rdfs:comment>
        <rdfs:label xml:lang="en">Compressive Strength</rdfs:label>
        <rdfs:label xml:lang="es">Resistencia a la Compresión</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#DRMSTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#DRMSTest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#NonDestructiveTest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#ForceMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#PenetrationDepthMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Drilling Resistance Measurement System (DRMS) investigates material strength by measuring the energy required to penetrate compact materials</rdfs:comment>
        <rdfs:label xml:lang="en">DRMS Test</rdfs:label>
        <rdfs:label xml:lang="es">Ensayo DRMS</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#Displacement -->


    <owl:Class rdf:about="https://w3id.org/ndto#Displacement">
        <rdfs:subClassOf rdf:resource="http://qudt.org/schema/qudt/Quantity"/>
        <qudt:quantityKind rdf:resource="http://qudt.org/vocab/quantitykind/Displacement"/>
        <rdfs:comment xml:lang="es">Desplazamiento</rdfs:comment>
        <rdfs:comment xml:lang="en">Displacement</rdfs:comment>
        <rdfs:label xml:lang="es">Desplazamiento</rdfs:label>
        <rdfs:label xml:lang="en">Displacement</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#DisplacementMeasurement -->


    <owl:Class rdf:about="https://w3id.org/ndto#DisplacementMeasurement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#NDTMeasurement"/>
        <rdfs:comment xml:lang="en">Displacement measurement</rdfs:comment>
        <rdfs:label xml:lang="es">Desplazamiento</rdfs:label>
        <rdfs:label xml:lang="en">Displacement Measurement</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#DoubleFlatJackTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#DoubleFlatJackTest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#FlatJackTest"/>
        <rdfs:comment xml:lang="en">Double flat-jack tests isolate a masonry specimen between two flat-jacks to perform in-situ compression testing</rdfs:comment>
        <rdfs:label xml:lang="en">Double Flat-Jack Test</rdfs:label>
        <rdfs:label xml:lang="es">Ensayo de Gato Plano Doble</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#ElasticWavePropagationTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#ElasticWavePropagationTest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#NonDestructiveTest"/>
        <rdfs:comment xml:lang="en">Elastic wave methods are based on the propagation of mechanical waves through materials to assess their internal structure and properties</rdfs:comment>
        <rdfs:label xml:lang="en">Elastic Wave Test</rdfs:label>
        <rdfs:label xml:lang="es">Ensayo de Ondas Elásticas</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#FlatJackTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#FlatJackTest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#NonDestructiveTest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#DisplacementMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#PressureMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Flat-jack tests determine compressive stress levels (single flat-jack) and mechanical properties (double flat-jack) of masonry walls</rdfs:comment>
        <rdfs:label xml:lang="es">Ensayo de Gato Plano</rdfs:label>
        <rdfs:label xml:lang="en">Flat-Jack Test</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#Force -->


    <owl:Class rdf:about="https://w3id.org/ndto#Force">
