    <!-- https://w3id.org/ndto#NonDestructiveTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#NonDestructiveTest">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:comment xml:lang="en">Non-destructive testing methods allow for the investigation of material structural properties which can be applied without any interventions in the object to be examined</rdfs:comment>
        <rdfs:label xml:lang="es">Ensayo No Destructivo</rdfs:label>
        <rdfs:label xml:lang="en">Non-Destructive Test</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#PWaveVelocityMeasurement -->


    <owl:Class rdf:about="https://w3id.org/ndto#PWaveVelocityMeasurement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#WaveVelocityMeasurement"/>
        <rdfs:comment xml:lang="en">Primary wave velocity (compression wave) measurement</rdfs:comment>
        <rdfs:label xml:lang="en">P-Wave Velocity</rdfs:label>
        <rdfs:label xml:lang="es">Velocidad de Onda P</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ndto#PendulumHammerTest -->


    <owl:Class rdf:about="https://w3id.org/ndto#PendulumHammerTest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ndto#SurfaceHardnessTest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ndto#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ndto#ReboundNumberMeasurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Uses a pendulum hammer to measure surface hardness through impact energy</rdfs:comment>
        <rdfs:label xml:lang="es">Ensayo de Martillo Pendular</rdfs:label>
        <rdfs:label xml:lang="en">Pendulum Hammer Test</rdfs:label>
    </owl:Class>
    


