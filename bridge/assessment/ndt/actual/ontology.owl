    <!-- https://w3id.org/ndto#hasMaterialProperty -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ndto#hasMaterialProperty">
        <rdfs:range rdf:resource="https://w3id.org/ndto#MaterialProperty"/>
        <rdfs:comment xml:lang="en">relates a Material with its mechanical properties, normally those evaluated in the context of an NDT</rdfs:comment>
        <rdfs:label xml:lang="en">is made of</rdfs:label>
        <rdfs:label xml:lang="es">is made of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ndto#hasMeasurement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ndto#hasMeasurement">
        <rdfs:domain rdf:resource="https://w3id.org/ndto#Material"/>
        <rdfs:domain rdf:resource="https://w3id.org/ndto#NonDestructiveTest"/>
        <rdfs:range rdf:resource="https://w3id.org/ndto#NDTMeasurement"/>
        <rdfs:comment xml:lang="en">relates a test with the measurements realized</rdfs:comment>
        <rdfs:label xml:lang="en">has measurement</rdfs:label>
        <rdfs:label xml:lang="es">tiene medición</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ndto#isMadeOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ndto#isMadeOf">
        <rdfs:range rdf:resource="https://w3id.org/ndto#Material"/>
        <rdfs:comment xml:lang="en">relates anything with the material it is  made of in the context of an NDT</rdfs:comment>
        <rdfs:label xml:lang="en">is made of</rdfs:label>
        <rdfs:label xml:lang="es">is made of</rdfs:label>
    </owl:ObjectProperty>
    


