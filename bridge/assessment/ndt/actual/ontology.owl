    <owl:Class rdf:about="https://w3id.org/ndto#NDTMeasurement">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
        <rdfs:comment xml:lang="en">Measurements realized in NDT procedures</rdfs:comment>
        <rdfs:label xml:lang="es">Medición de Ensayo No Destructivo</rdfs:label>
        <rdfs:label xml:lang="en">NDT Measurement</rdfs:label>
