@prefix : <https://w3id.org/ndto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/ndto#> .

<https://w3id.org/ndto#> rdf:type owl:Ontology ;
                          owl:versionIRI <https://w3id.org/ndto#> ;
                          <http://creativecommons.org/ns#license> "http://creativecommons.org/licenses/by/3.0/" ;
                          <http://purl.org/dc/elements/1.1/creator> "Carlos Ramonell Cazador (carlos.ramonell@upc.edu)" ;
                          <http://purl.org/dc/elements/1.1/date> "2025-07-14" ;
                          <http://purl.org/dc/elements/1.1/description> "Ontology that describes different non-destructive testing techniques and their applications for structural assessment"@en ,
                                                                        "Ontología que describe diferentes técnicas de ensayo no destructivas y sus aplicaciones para la evaluación estructural"@es ;
                          <http://purl.org/dc/elements/1.1/language> "en" ,
                                                                     "es" ;
                          <http://purl.org/dc/elements/1.1/title> "Non-Destructive Testing Ontology"@en ,
                                                                  "Ontología de Técnicas de Ensayo No Destructivas"@es ;
                          <http://purl.org/vocab/vann/preferredNamespacePrefix> "ndt" ;
                          <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/ndto#" ;
                          owl:versionInfo "1.1" .

